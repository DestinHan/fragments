{
  "name": "fragments",
  "private": true,
  "version": "0.7.0",
  "description": "Fragments back-end API",
  "scripts": {
    "lint": "eslint \"./src/**/*.js\" \"tests/**/*.js\"",
    "test": "cross-env AUTH_STRATEGY=http jest --runInBand",
    "test:watch": "cross-env AUTH_STRATEGY=http jest --watch",
    "coverage": "cross-env AUTH_STRATEGY=http jest --runInBand --coverage --coverageReporters=text",
    "start": "node src/server.js",
    "dev": "node --env-file=debug.env --watch ./src/server.js",
    "debug": "node --env-file=debug.env --inspect=0.0.0.0:9229 --watch ./src/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DestinHan/fragments.git"
  },
  "author": "Seung Hoon Han",
  "license": "UNLICENSED",
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.34.0",
    "globals": "^16.3.0",
    "jest": "^30.1.3",
    "prettier": "3.6.2",
    "supertest": "^7.1.4"
  },
  "dependencies": {
    "aws-jwt-verify": "^5.1.0",
    "compression": "^1.8.1",
    "content-type": "^1.0.5",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "passport": "^0.7.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "pino": "^9.9.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.1.1",
    "stoppable": "^1.1.0"
  }
}
